    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "args": [
                "-t",
                "\"hello {{var: User}} {{var: Id}}. Numbers: {{join: Numbers, \", \"}}.\"",
                "-v",
                "User=\"My Name\"",
                "-v",
                "Id=\"1999\"",
                "-j",
                "Numbers=\"1,2,3\""
            ],
            "mode": "auto",
            "program": "${fileDirname}"
        }
    ]
}